{
	"name": "fpubsub",
	"version": "0.9.4",
	"description": "PubSub pattern library with types support",
	"keywords": [
		"event",
		"event-emitter",
		"reactive",
		"reactive-programming",
		"functional-programming",
		"pubsub",
		"pub-sub",
		"javascript",
		"typescript"
	],
	"author": "Jan Andrle <andrle.jan@centrum.cz>",
	"license": "MIT",
	"homepage": "https://github.com/jaandrle/fpubsub",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaandrle/fpubsub.git"
	},
	"bugs": {
		"url": "https://github.com/jaandrle/fpubsub/issues"
	},
	"scripts": {
		"build:lint": "npx jshint **/*.js --show-non-errors",
		"build:doc": "npx typedoc dist/esm.d.ts --readme none --includeVersion --sort visibility",
		"build": "npm run build:lint && npm run build:doc"
	},
	"main": "dist/esm.js",
	"types": "dist/esm.d.ts",
	"exports": {
		".": "./dist/esm.js"
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/esm.d.ts"
			]
		}
	},
	"files": [
		"dist/esm.js",
		"dist/esm.d.ts"
	],
	"type": "module",
	"engines": {
		"node": ">=14.17"
	},
	"jshintConfig": {
		"esversion": 9,
		"browser": "true",
		"node": "true",
		"undef": "true",
		"latedef": "true",
		"maxparams": 4,
		"maxdepth": 3,
		"maxcomplexity": 17,
		"globals": {
			"globalThis": false,
			"AbortSignal": false
		}
	},
	"devDependencies": {
		"jshint": "^2.13.6",
		"typedoc": "^0.23.23",
		"typedoc-plugin-markdown": "^3.14.0"
	}
}
