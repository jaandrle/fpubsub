{
	"name": "fpubsub",
	"version": "0.9.0",
	"description": "PubSub pattern library with types support",
	"keywords": [
		"event",
		"event-emitter",
		"reactive",
		"reactive-programming",
		"functional-programming",
		"pubsub",
		"pub-sub",
		"javascript",
		"typescript"
	],
	"author": "Jan Andrle <andrle.jan@centrum.cz>",
	"license": "MIT",
	"homepage": "https://github.com/jaandrle/fpubsub",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaandrle/fpubsub.git"
	},
	"bugs": {
		"url": "https://github.com/jaandrle/fpubsub/issues"
	},
	"scripts": {
		"doc": "npx typedoc index.d.ts --readme none --defaultCategory 'Internal' --categoryOrder 'Public' --categoryOrder 'Internal' --sort visibility --disableSources",
		"lint": "npx jshint **/*.js --show-non-errors"
	},
	"main": "index.js",
	"types": "index.d.ts",
	"exports": {
		".": "index.js"
	},
	"typesVersions": {
		"*": {
			".": [
				"./index.d.ts"
			]
		}
	},
	"files": [
		"index.js",
		"index.d.ts",
		"dist"
	],
	"type": "module",
	"engines": {
		"node": ">=14.17"
	},
	"jshintConfig": {
		"esversion": 6,
		"browser": "true",
		"node": "true",
		"undef": "true",
		"latedef": "true",
		"maxparams": 4,
		"maxdepth": 3,
		"maxcomplexity": 17,
		"globals": {
			"globalThis": false
		}
	},
	"devDependencies": {
		"jshint": "^2.13.6",
		"typedoc": "^0.23.23",
		"typedoc-plugin-markdown": "^3.14.0"
	}
}
