{
	"name": "fpubsub",
	"version": "0.9.4",
	"description": "PubSub pattern library with types support",
	"keywords": [
		"event",
		"event-emitter",
		"reactive",
		"reactive-programming",
		"functional-programming",
		"pubsub",
		"pub-sub",
		"javascript",
		"typescript"
	],
	"author": "Jan Andrle <andrle.jan@centrum.cz>",
	"license": "MIT",
	"homepage": "https://github.com/jaandrle/fpubsub",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaandrle/fpubsub.git"
	},
	"bugs": {
		"url": "https://github.com/jaandrle/fpubsub/issues"
	},
	"scripts": {
		"build:cjs": "npx nodejsscript scripts/build.js",
		"build:lint": "npx jshint lib/*.js --show-non-errors",
		"build:doc": "npx typedoc lib/esm.d.ts --readme none --includeVersion --sort visibility",
		"build": "clear && npm run build:cjs && npm run build:lint && npm run build:doc"
	},
	"main": "lib/cjs.js",
	"module": "lib/esm.js",
	"types": "lib/esm.d.ts",
	"exports": {
		".": {
			"import": "./lib/esm.js",
			"browser": "./lib/cjs.js",
			"require": "./lib/cjs.js"
		}
	},
	"typesVersions": {
		"*": {
			".": [
				"./lib/esm.d.ts"
			]
		}
	},
	"files": [ "lib" ],
	"type": "module",
	"engines": {
		"node": ">=14.17"
	},
	"jshintConfig": {
		"esversion": 9,
		"browser": "true",
		"node": "true",
		"undef": "true",
		"latedef": "true",
		"maxparams": 4,
		"maxdepth": 3,
		"maxcomplexity": 8,
		"globals": {
			"globalThis": false,
			"AbortSignal": false
		}
	},
	"devDependencies": {
		"jshint": "^2.13.6",
		"nodejsscript": "^0.9.0",
		"typedoc": "^0.23.23",
		"typedoc-plugin-markdown": "^3.14.0"
	}
}
